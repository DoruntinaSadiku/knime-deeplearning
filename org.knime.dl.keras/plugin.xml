<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.dl.DLExecutionContext">
      <DLExecutionContext
            DLExecutionContext="org.knime.dl.keras.cntk.core.execution.DLKerasCNTKDefaultExecutionContext">
      </DLExecutionContext>
      <DLExecutionContext
            DLExecutionContext="org.knime.dl.keras.tensorflow.core.execution.DLKerasTensorFlowDefaultExecutionContext">
      </DLExecutionContext>
      <DLExecutionContext
            DLExecutionContext="org.knime.dl.keras.theano.core.execution.DLKerasTheanoDefaultExecutionContext">
      </DLExecutionContext>
   </extension>
   <extension
         point="org.knime.dl.DLLayerDataFactory">
      <DLLayerDataFactory
            DLLayerDataFactory="org.knime.dl.keras.cntk.core.DLKerasCNTKLayerDataFactory">
      </DLLayerDataFactory>
      <DLLayerDataFactory
            DLLayerDataFactory="org.knime.dl.keras.tensorflow.core.DLKerasTensorFlowLayerDataFactory">
      </DLLayerDataFactory>
      <DLLayerDataFactory
            DLLayerDataFactory="org.knime.dl.keras.theano.core.DLKerasTheanoLayerDataFactory">
      </DLLayerDataFactory>
   </extension>
   <extension
         point="org.knime.dl.DLNetworkType">
      <DLNetworkType
            DLNetworkType="org.knime.dl.keras.cntk.core.DLKerasCNTKNetworkType">
      </DLNetworkType>
      <DLNetworkType
            DLNetworkType="org.knime.dl.keras.tensorflow.core.DLKerasTensorFlowNetworkType">
      </DLNetworkType>
      <DLNetworkType
            DLNetworkType="org.knime.dl.keras.theano.core.DLKerasTheanoNetworkType">
      </DLNetworkType>
   </extension>
   <extension
         point="org.knime.python2.modules">
      <directory
            path="py/">
      </directory>
   </extension>
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/category.png"
            level-id="keras"
            locked="true"
            name="Keras"
            path="/labs/deeplearning">
      </category>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/labs/deeplearning/keras"
            deprecated="false"
            factory-class="org.knime.dl.keras.base.nodes.reader.DLKerasReaderNodeFactory">
      </node>
  </extension>
</plugin>
