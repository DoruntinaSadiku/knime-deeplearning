<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="../dlkerasconvlayer.png" type="Other">
	<name>Keras Upsampling 1D Layer</name>

	<shortDescription>
		Keras Upsampling 1D Layer
	</shortDescription>

	<fullDescription>
		<intro>
			This node adds a
			<a href="https://keras.io/layers/convolutional/#upsampling1d">Keras Upsampling 1D Layer</a>
			to an existing deep learning network.
		</intro>
		<tab name="Options">
			<option name="Size">
				Upsampling factor.
			</option>
		</tab>
	</fullDescription>
	<ports>
		<inPort index="0" name="Deep Learning Network">
			The network to append the layer to.
		</inPort>
		<outPort index="0" name="Deep Learning Network">
			The network with appended layer.
		</outPort>
	</ports>
</knimeNode>
