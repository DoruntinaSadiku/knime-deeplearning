<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="dlkerasflattenlayer.png" type="Other">
	<name>Keras Reshape Layer</name>

	<shortDescription>
		A reshape layer.
	</shortDescription>

	<fullDescription>
		<intro>
			This node adds a
			<a href="https://keras.io/layers/core/#reshape">Keras Reshape Layer</a>
			to an existing deep learning network.
		</intro>
		<tab name="Options">
			<option name="Shape">
				Tuple of integers defining the target shape.
			</option>
		</tab>
	</fullDescription>
	<ports>
		<inPort index="0" name="Deep Learning Network">
			The network to append the layer to.
		</inPort>
		<outPort index="0" name="Deep Learning Network">
			The network with appended layer.
		</outPort>
	</ports>
</knimeNode>
